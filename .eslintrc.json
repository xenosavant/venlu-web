{
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "extends": [
        "airbnb",
        "airbnb-typescript"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": [
            "tsconfig.json"
        ]
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "no-param-reassign": "off",
        "noUnusedExpressions": {
            "allowTernary": true
        },
        "@typescript-eslint/explicit-function-return-type": "off",
        "react/require-default-props": "off"
    }
}